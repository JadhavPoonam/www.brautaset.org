---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>SuperLoopy</title>
  <link href="http://superloopy.io"/>
  <link type="application/atom+xml" rel="self" href="http://superloopy.io/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://superloopy.io</id>
  <author>
    <name>Stig Brautaset</name>
    <email>stig@brautaset.org</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://superloopy.io/{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://superloopy.io/{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
